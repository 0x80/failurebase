    "eslint-plugin-require-extensions": "^0.1.3",
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "firebase": "^10.0",
    "react": ">=16.8.0"
